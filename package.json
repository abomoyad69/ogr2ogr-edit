{
  "name": "ogr2ogr-edit",
  "version": "0.0.1",
  "description": "ogr2ogr wrapper w/ multiple format support",
  "keywords": [
    "ogr2ogr",
    "stream",
    "proj4",
    "gdal",
    "Al-Jezawi"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/abomoyad69/ogr2ogr-edit.git"
  },
  "homepage": "https://github.com/abomoyad69/ogr2ogr-edit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc",
    "build": "tsc",
    "test": "nyc ts-node node_modules/.bin/blue-tape \"**/*_test.ts\"",
    "lint": "tsc --noEmit && eslint --ext ts .",
    "fmt": "prettier --write \"**/*.{html,js,json,ts,md}\"",
    "fmt-check": "prettier --check \"**/*.{html,js,json,ts,md}\""
  },
  "dependencies": {
    "archiver": "^5.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/blue-tape": "^0.1.33",
    "@types/geojson": "^7946.0.7",
    "@types/node": "^16.4.5",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "blue-tape": "^1.0.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^2.3.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "ISC"
}
